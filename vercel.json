{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ctd-learns-node-l42tx.ondigitalocean.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "X-Same-Origin-Proxy",
          "value": "vercel"
        }
      ]
    }
  ]
}
