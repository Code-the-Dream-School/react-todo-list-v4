{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/remote-api/:path*",
      "destination": "https://ctd-learns-node-l42tx.ondigitalocean.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/remote-api/:path*",
      "headers": [
        {
          "key": "X-Same-Origin-Proxy",
          "value": "vercel"
        }
      ]
    }
  ]
}